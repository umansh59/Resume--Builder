{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node",
      
    }
  ],
  "routes": [
    {
      "source": "/(.*)",
      "destination": "/index.js",
      "methods":["GET", "POST","PUT","DELETE", "PATCH","OPTIONS"],
      "headers":{
        "Acess-Control-Allow-Origin":"*"
      }
    }
  ]
 
}
